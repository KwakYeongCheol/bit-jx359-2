<!DOCTYPE sqlMap
    PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="PostMetadata">
	<typeAlias alias="PostMetadata" type="kr.co.webcash.domain.post.PostMetadata"/>
	
	<select id="findByPostId" resultClass="PostMetadata">
		SELECT
			postId,
			isPublic,
			canScrap,
			canTrackback
		FROM
			post_metadata
		WHERE
			postId = #postId#
	</select>
	
	
</sqlMap>